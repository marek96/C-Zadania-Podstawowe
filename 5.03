using namespace std;

void pole_pr(float a,float b)
{
    float pole;

    pole = a * b;
    cout << "Pole prostokata wynosi: " << setprecision(4) <<pole << endl;
}

int main()
{
    int x1,x2;
    cout << "Wprowadz bok a: ";
    cin >> x1;
    cout << "Wprowadz bok b: ";
    cin >> x2;

    pole_pr(x1,x2);
    system("PAUSE");
    return 0;
}
